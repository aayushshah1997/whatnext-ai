{
  "name": "bolt-expo-starter",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "EXPO_NO_TELEMETRY=1 expo start",
    "build:web": "expo export --platform web",
    "lint": "expo lint",
    "start:dev": "expo start --tunnel",
    "start:clear": "expo start --tunnel --clear",
    "start:ios": "EXPO_NO_TELEMETRY=1 expo start --tunnel --ios",
    "start:android": "EXPO_NO_TELEMETRY=1 expo start --tunnel --android",
    "start:lan": "CONNECTION_MODE=lan expo start --lan",
    "start:local": "CONNECTION_MODE=local expo start --localhost",
    "start:reset": "expo start --clear --tunnel && rm -rf node_modules/.cache",
    "setup": "./setup-expo.sh"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo/vector-icons": "^14.1.0",
    "@lucide/lab": "^0.1.2",
