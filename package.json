{
  "name": "bolt-expo-starter",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "EXPO_NO_TELEMETRY=1 expo start",
    "build:web": "expo export --platform web",
    "lint": "expo lint",
    "start:dev": "expo start --tunnel",
    "start:clear": "expo start --tunnel --clear",
    "start:ios": "EXPO_NO_TELEMETRY=1 expo start --tunnel --ios",
    "start:android": "EXPO_NO_TELEMETRY=1 expo start --tunnel --android",
    "start:lan": "CONNECTION_MODE=lan expo start --lan",
    "start:local": "CONNECTION_MODE=local expo start --localhost",
    "start:reset": "expo start --clear --tunnel && rm -rf node_modules/.cache",
    "setup": "./setup-expo.sh"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo/vector-icons": "^14.0.2",
    "@lucide/lab": "^0.1.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "expo": "52.0.33",
    "expo-blur": "^14.0.3",
    "expo-constants": "^17.0.5",
    "expo-font": "^13.0.3",
    "expo-haptics": "^14.0.1",
    "expo-linear-gradient": "^14.0.2",
    "expo-linking": "^7.0.5",
    "expo-location": "~16.1.0",
    "expo-network": "^7.0.5",
    "expo-splash-screen": "^0.29.21",
    "expo-status-bar": "^2.0.1",
    "expo-symbols": "^0.2.2",
    "expo-system-ui": "^4.0.7",
    "expo-web-browser": "^14.0.2",
    "lucide-react-native": "^0.475.0",
    "matter-js": "^0.20.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.6",
    "react-native-game-engine": "^1.2.0",
    "react-native-gesture-handler": "^2.23.0",
    "react-native-reanimated": "^3.16.7",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "^4.4.0",
    "react-native-svg": "^15.11.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.19.13",
    "react-native-webview": "13.12.5",
    "together-ai": "^0.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@types/matter-js": "^0.19.8",
    "@types/react": "~18.3.12",
    "@types/react-native": "^0.72.8",
    "dotenv": "^16.5.0",
    "expo-camera": "^16.0.18",
    "expo-face-detector": "^13.0.2",
    "expo-router": "^4.0.20",
    "react-native-dotenv": "^3.4.11",
    "typescript": "^5.3.3"
  }
}
